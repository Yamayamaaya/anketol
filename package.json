{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "main": "firebaseFunctions.js",
  "scripts": {
    "dev": "run-p dev:* watch:*",
    "dev:next": "next dev",
    "build": "next build && next export",
    "export": "yarn build && serve ./out",
    "build:next": "next build",
    "start": "next start",
    "lint": "run-p lint:*",
    "lint:next": "next lint . --ignore-path .prettierignore --max-warnings 0",
    "lint:prettier": "prettier --check .",
    "lint:typecheck": "tsc --pretty --noEmit",
    "format": " format:eslint format:prettier",
    "format:eslint": "yarn lint:next --fix",
    "format:prettier": "yarn lint:prettier --write",
    "watch:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida --watch",
    "generate:path": "pathpida --ignorePath .gitignore --output src/lib/pathpida",
    "login": "firebase login",
    "serve": "next build && next export && firebase emulators:start --only functions,hosting",
    "shell": "next build && firebase functions:shell",
    "deploy": "next build && next export && firebase deploy --only functions,hosting",
    "logs": "firebase functions:log"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.6",
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "firebase": "^10.7.1",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.5.0",
    "firebase-tools": "^13.0.2",
    "framer-motion": "^7.5.4",
    "next": "13.1.2",
    "pathpida": "^0.18.1",
    "react": "18.2.0",
    "react-datepicker": "^4.24.0",
    "react-dom": "18.2.0",
    "utility-types": "^3.10.0"
  },
  "devDependencies": {
    "@tsconfig/strictest": "^1.0.2",
    "@types/node": "18.11.0",
    "@types/react": "18.0.21",
    "@types/react-datepicker": "^4.19.3",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.16",
    "eslint": "8.25.0",
    "eslint-config-next": "13.1.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.32",
    "prettier": "^2.7.1",
    "serve": "^14.2.1",
    "tailwindcss": "^3.3.6",
    "typescript": "4.8.4"
  },
  "volta": {
    "node": "16.18.0",
    "yarn": "1.22.19"
  },
  "engines": {
    "node": "18.x"
  }
}
